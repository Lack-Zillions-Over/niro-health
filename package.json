{
  "name": "niro-health",
  "version": "1.6.0-alpha.6",
  "license": "MIT",
  "engines": {
    "yarn": ">=1.22.19",
    "npm": "USE YARN INSTEAD OF NPM TO INSTALL DEPENDENCIES",
    "node": ">=18.14.0 <=19.0.0"
  },
  "repository": "git://github.com/Lack-Zillions-Over/niro-health.git",
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@nestjs/cli": "^9.2.0",
    "@nestjs/schematics": "^9.0.4",
    "@nestjs/testing": "^9.3.9",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.17",
    "@types/fs-extra": "^11.0.1",
    "@types/geoip-lite": "^1.4.1",
    "@types/jest": "29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/lz-string": "^1.3.34",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.15.0",
    "@types/nodemailer": "^6.4.7",
    "@types/pako": "^2.0.0",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "cpx": "^1.5.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "29.5.0",
    "lint-staged": "^13.2.0",
    "prettier": "^2.8.4",
    "prisma": "^4.11.0",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.3",
    "typescript": "^4.9.5",
    "webpack": "^5.76.0",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "commit": "cz",
    "prerelease": "standard-version --prerelease alpha && yarn postrelease",
    "release": "standard-version && yarn postrelease",
    "postrelease": "git push --follow-tags origin main",
    "audit-fix": "npx yarn-audit-fix",
    "update": "npx yarn upgrade-interactive --latest",
    "project-copy-config": "cpx \"src/project.config.json\" dist",
    "project-copy-locales": "cpx \"src/core/libs/locales/*.json\" dist/core/libs/locales",
    "project-copy-email": "cpx \"src/core/libs/templates/*.pug\" dist/core/libs/templates",
    "project-copy-files": "yarn project-copy-config && yarn project-copy-locales && yarn project-copy-email",
    "prebuild": "rimraf dist",
    "build": "nest build && yarn project-copy-files",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "yarn migrate-deploy && node dist/main",
    "migrate-deploy": "yarn prisma migrate deploy",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node -r dotenv/config $(which jest) --detectOpenHandles --forceExit --no-cache",
    "test:watch": "node -r dotenv/config $(which jest) --watchAll --detectOpenHandles --forceExit --no-cache",
    "test:cov": "node -r dotenv/config $(which jest) --coverage --detectOpenHandles --forceExit --no-cache",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json --detectOpenHandles --forceExit --no-cache"
  },
  "dependencies": {
    "@bull-board/api": "^5.0.0",
    "@bull-board/express": "^5.0.0",
    "@nestjs/axios": "^2.0.0",
    "@nestjs/bull": "^0.6.3",
    "@nestjs/common": "^9.3.9",
    "@nestjs/config": "^2.3.1",
    "@nestjs/core": "^9.3.9",
    "@nestjs/mapped-types": "1.2.2",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.3.9",
    "@nestjs/platform-socket.io": "^9.3.9",
    "@nestjs/schedule": "^2.2.0",
    "@nestjs/websockets": "^9.3.9",
    "@prisma/client": "^4.11.0",
    "@sequelize/core": "^7.0.0-alpha.23",
    "@socket.io/redis-adapter": "^8.1.0",
    "@types/archiver": "^5.3.1",
    "archiver": "^5.3.1",
    "aws-sdk": "^2.1332.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.4",
    "bull-board": "^2.1.3",
    "cookie-parser": "^1.4.6",
    "express-basic-auth": "^1.2.1",
    "geoip-lite": "^1.4.7",
    "joi": "^17.8.3",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "lz-string": "^1.5.0",
    "lzutf8": "^0.6.3",
    "moment": "^2.29.4",
    "mongoose": "^7.0.1",
    "nodemailer": "^6.9.1",
    "nodemailer-pug-engine": "^2.0.1",
    "pako": "^2.1.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "pino": "^8.11.0",
    "pino-pretty": "^9.4.0",
    "redis": "^4.6.5",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^4.4.0",
    "rxjs": "^7.8.0",
    "socket.io": "^4.6.1",
    "sugar": "^2.0.6",
    "uuid": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": "tsc-files --noEmit",
    "src/**/*": [
      "yarn lint",
      "yarn format"
    ]
  },
  "bugs": {
    "url": "https://github.com/Lack-Zillions-Over/niro-health/issues"
  },
  "homepage": "https://github.com/Lack-Zillions-Over/niro-health#readme",
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "setupFiles": [
      "<rootDir>/test/setup-test.ts"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1",
      "^@test/mocks(|/.*)$": "<rootDir>/test/__mocks__/$1",
      "^@app/crypto(|/.*)$": "<rootDir>/libs/crypto/src/$1",
      "^@app/i18n(|/.*)$": "<rootDir>/libs/i18n/src/$1",
      "^@app/random(|/.*)$": "<rootDir>/libs/random/src/$1",
      "^@app/string-ex(|/.*)$": "<rootDir>/libs/string-ex/src/$1",
      "^@app/prop-string(|/.*)$": "<rootDir>/libs/prop-string/src/$1",
      "^@app/bjson(|/.*)$": "<rootDir>/libs/bjson/src/$1",
      "^debug/pino(|/.*)$": "<rootDir>/libs/pino/src/$1",
      "^@app/debug(|/.*)$": "<rootDir>/libs/debug/src/$1",
      "^@app/redis(|/.*)$": "<rootDir>/libs/redis/src/$1",
      "^@app/archive(|/.*)$": "<rootDir>/libs/archive/src/$1",
      "^@app/hyperc(|/.*)$": "<rootDir>/libs/hyperc/src/$1",
      "^@app/json-web-token(|/.*)$": "<rootDir>/libs/json-web-token/src/$1",
      "^@app/email(|/.*)$": "<rootDir>/libs/email/src/$1",
      "^@app/aws-configuration(|/.*)$": "<rootDir>/libs/aws-configuration/src/$1",
      "^@app/similarity-filter(|/.*)$": "<rootDir>/libs/similarity-filter/src/$1",
      "^@app/aws-ec2(|/.*)$": "<rootDir>/libs/aws-ec2/src/$1",
      "^@app/aws-sts(|/.*)$": "<rootDir>/libs/aws-sts/src/$1",
      "^@app/configuration(|/.*)$": "<rootDir>/libs/configuration/src/$1",
      "^@app/bootstrap(|/.*)$": "<rootDir>/libs/bootstrap/src/$1",
      "^@app/validator-regexp(|/.*)$": "<rootDir>/libs/validator-regexp/src/$1"
    },
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/",
      "<rootDir>/test/"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
