{
  "name": "niro-health",
  "version": "1.5.1",
  "engines": {
    "yarn": ">=1.22.19",
    "npm": ">=8.19.2 <=9.0.0",
    "node": ">=18.12.1 <=19.0.0"
  },
  "repository": "git://github.com/Lack-Zillions-Over/niro-health.git",
  "devDependencies": {
    "@jest-mock/express": "^2.0.1",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.14",
    "@types/fs-extra": "^9.0.13",
    "@types/geoip-lite": "^1.4.1",
    "@types/jest": "29.2.4",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/lodash": "^4.14.191",
    "@types/lz-string": "^1.3.34",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.13",
    "@types/pako": "^2.0.0",
    "@types/request-ip": "^0.0.37",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "cpx": "^1.5.0",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.2",
    "jest": "29.3.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "prisma": "^4.7.1",
    "run-script-webpack-plugin": "^0.1.1",
    "source-map-support": "^0.5.20",
    "standard-version": "^9.5.0",
    "supertest": "^6.3.3",
    "ts-jest": "29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsc-files": "^1.1.3",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-node-externals": "^3.0.0"
  },
  "scripts": {
    "commit": "cz",
    "release": "standard-version && git push --follow-tags origin main",
    "audit-fix": "npx yarn-audit-fix",
    "update": "npx yarn upgrade-interactive --latest",
    "project-copy-config": "cpx \"src/project.config.json\" dist",
    "project-copy-locales": "cpx \"src/core/libs/locales/*.json\" dist/core/libs/locales",
    "project-copy-email": "cpx \"src/core/libs/templates/*.pug\" dist/core/libs/templates",
    "project-copy-files": "yarn project-copy-config && yarn project-copy-locales && yarn project-copy-email",
    "prebuild": "rimraf dist",
    "build": "nest build && yarn project-copy-files",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest build --webpack --webpackPath webpack-hmr.config.js --watch",
    "start:debug": "nest start --debug 0.0.0.0:9229 --watch",
    "start:prod": "yarn migrate-deploy && node dist/main",
    "migrate-deploy": "yarn prisma migrate deploy",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "node -r dotenv/config $(which jest)",
    "test:watch": "node -r dotenv/config $(which jest) --watch",
    "test:cov": "node -r dotenv/config $(which jest) --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@bull-board/api": "^4.8.0",
    "@bull-board/express": "^4.8.0",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/mapped-types": "1.2.0",
    "@nestjs/mongoose": "^9.2.1",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/platform-socket.io": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/websockets": "^9.2.1",
    "@prisma/client": "^4.7.1",
    "@sequelize/core": "^7.0.0-alpha.10",
    "@socket.io/redis-adapter": "^8.0.0",
    "@types/archiver": "^5.3.1",
    "archiver": "^5.3.1",
    "aws-sdk": "^2.1273.0",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.2",
    "bull-board": "^2.1.3",
    "cookie-parser": "^1.4.6",
    "express-basic-auth": "^1.2.1",
    "geoip-lite": "^1.4.6",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "lz-string": "^1.4.4",
    "moment": "^2.29.4",
    "mongoose": "^6.8.0",
    "nodemailer": "^6.8.0",
    "nodemailer-pug-engine": "^2.0.1",
    "pako": "^2.1.0",
    "passport": "^0.6.0",
    "passport-http": "^0.3.0",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "pino": "^8.7.0",
    "pino-pretty": "^9.1.1",
    "redis": "^4.5.1",
    "reflect-metadata": "^0.1.13",
    "request-ip": "^3.3.0",
    "rimraf": "^3.0.2",
    "rxjs": "^7.6.0",
    "socket.io": "^4.5.4",
    "sugar": "^2.0.6",
    "uuid": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.ts": "tsc-files --noEmit",
    "src/**/*": [
      "yarn lint",
      "yarn format"
    ]
  },
  "bugs": {
    "url": "https://github.com/Lack-Zillions-Over/niro-health/issues"
  },
  "homepage": "https://github.com/Lack-Zillions-Over/niro-health#readme",
  "volta": {
    "node": "18.12.1",
    "yarn": "1.22.19"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
