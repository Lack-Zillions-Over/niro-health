{
  "locale": {
    "main": "en",
    "languages": [
      "en",
      "pt_br"
    ],
    "path": "locales"
  },
  "log": {
    "path": "logs"
  },
  "email": {
    "provider": "smtp"
  },
  "files": {
    "path": "uploads",
    "single": {
      "fieldname": "file",
      "mimetypes": [
        "image/png",
        "image/jpeg",
        "image/jpg",
        "image/gif",
        "image/webp",
        "image/bmp",
        "image/tiff",
        "image/ico"
      ],
      "limit": 5242880,
      "aws": {
        "bucket": "niro-health-single",
        "encryptAlgorithm": "AES256"
      }
    },
    "multiple": {
      "fieldname": "files",
      "mimetypes": [
        "image/png",
        "image/jpeg",
        "image/jpg",
        "image/gif",
        "image/webp",
        "image/bmp",
        "image/tiff",
        "image/ico"
      ],
      "limit": 5242880,
      "aws": {
        "bucket": "niro-health-multiple",
        "encryptAlgorithm": "AES256"
      },
      "maxFiles": 10
    }
  },
  "redis": {
    "database": 1
  },
  "aws": {
    "apiVersion": "latest",
    "region": "us-east-1",
    "httpsAgent": {
      "maxSockets": 25,
      "keepAlive": true,
      "keepAliveMsecs": 30000
    }
  }
}
